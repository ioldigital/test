                  "& when (@reverseContent)": {
                    "padding-left": "70px"
                  }
                },
                "@media (min-width: 1200px)": {
                  "& when (@fullWidth)": {
                    "padding-left": "100px",
                    "padding-right": "100px"
                  }
                }
              }
            },
            ".container-img when (@showImg)": {
              "padding": "0"
            },
            ".elements-block .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".marker-wrap:before, .dots span, .mbr-content-subtitle:before": {
              "display": "inline-block"
            },
            ".mbr-list when (@stylizedCounters = \"1\") and (@showCardList)": {
              "counter-reset": "decimal",
              ".marker-wrap:before": {
                "counter-increment": "decimal",
                "content": "\"0\" counter(decimal)",
                "color": "@primaryColor",
                "& when (@countersBorder)": {
                  "border-bottom": "2px solid @primaryColor"
                }
              }
            },
            ".marker-wrap:before when (@stylizedCounters = \"2\") and (@showCardList)": {
              "content": "\"\"",
              "border": "3px solid @primaryColor",
              "width": "~\"calc(@{widthImg}rem - 6px)\"",
              "height": "~\"calc(@{widthImg}rem - 6px)\"",
              "box-sizing": "initial"
            },
            ".mbr-list .items-list when (@showCardList)": {
              "flex-direction": "row",
              "& when not (@stylizedCounters = \"3\")": {
                "align-items": "baseline"
              },
              ".marker-wrap, .icon-wrap": {
                "margin-right": "15px"
              },
              "&:not(:last-child)": {
                "padding-bottom": "10px"
              },
              "& when ((@widthImg * 16) > 30)": {
                ".marker-wrap:before, .icon-wrap .amp-iconfont": {
                  "@media (max-width: 767px)": {
                    "font-size": "30px",
                    "width": "30px",
                    "height": "30px"
                  }
                }
              }
            },
            ".icon-wrap .amp-iconfont when (@stylizedCounters = \"3\") and (@showCardList)": {
              "color": "@primaryColor",
              "font-size": "~\"@{widthImg}rem\"",
              "width": "~\"@{widthImg}rem\""
            },
            "& when (@showImg), (@showImgText), (@showImgTitle)": {
              "amp-carousel .slide": {
                "align-self": "flex-start"
              },
              ".slide": {
                "height": "~\"calc(@{slideHeight}px - 35px)\"",
                "margin-bottom": "35px",
                "amp-img,img": {
                  "object-fit": "cover"
                },
                ".container-img img when (@showImg)": {
                  "-webkit-transition": "transform 5s linear",
                  "transition": "transform 5s linear"
                },
                "&:hover .container-img img when (@showImg)": {
                  "-webkit-transform": "scale(1.15)",
                  "transform": "scale(1.15)"
                }
              },
              ".dots-wrapper": {
                "position": "absolute",
                "bottom": "0"
              },
              ".dots span, .amp-carousel-button": {
                "background-color": "@buttonsColor"
              },
              ".dots span": {
                "border-radius": "10px",
                "width": "9px",
                "height": "9px",
                "margin": "0 3px",
                "opacity": "0.4",
                "outline": "none"
              },
              ".dots span.current": {
                "opacity": "1",
                "width": "18px"
              },
              ".amp-carousel-button": {
                "display": "none"
              },
              "& when not (@showBullets)": {
                "@media (min-width: 768px)": {
                  ".slide": {
                    "height": "~\"calc(@{slideHeight}px - 85px)\"",
                    "margin-bottom": "85px"
                  },
                  ".amp-carousel-button": {
                    "display": "block",
                    "top": "~\"calc(100% - 27px)\"",
                    "opacity": "0.8",
                    "&:after": {
                      "border-color": "contrast(@buttonsColor, darken(@buttonsColor, 50%), #ffffff, 60%) !important"
                    },
                    "&:hover": {
                      "opacity": "1"
                    }
                  },
                  ".amp-carousel-button-prev": {
                    "left": "0px"
                  },
                  ".amp-carousel-button-next": {
                    "left": "55px"
                  },
                  ".dots-wrapper": {
                    "display": "none"
                  }
                }
              }
            },
            "@media (max-width: 767px)": {
              ".container-img when (@showImgText), (@showImgTitle)": {
                "height": "50%"
              },
              ".text-wrap when (@showImg)": {
                "height": "50%"
              },
              ".second-element when (@showImgText), (@showImgTitle), (@showImg)": {
                "padding-bottom": "50px"
              },
              ".text-wrap when (@showImg) and (@testimonialsBg)": {
                "padding-top": "25px"
              }
            }
          },
          "_name": "slider5",
          "_customHTML": "<section class=\"slider5 slider slider-with-bullets\" group=\"Testimonials\" plugins=\"amp-main, amp-carousel, amp-bind\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline=\"\" title=\"Width Images\" name=\"columnWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"(showCardTitle || showCardSubtitle || showCardList || showCardText || showCardButtons) &amp;&amp; (showImgText || showImgTitle || showImg)\">    \n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"(showCardTitle || showCardSubtitle || showCardList || showCardText || showCardButtons) &amp;&amp; (showImgText || showImgTitle || showImg)\">\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>    \n    <header>Testimonials</header>\n        <input type=\"range\" inline=\"\" title=\"Width Testimonials\" name=\"testimonialsWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"(showImgText || showImgTitle) &amp;&amp; showImg\">\n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"testimonialsReverseContent\" condition=\"(showImgText || showImgTitle) &amp;&amp; showImg\">\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\" checked=\"\" condition=\"imagesAmount > 1 &amp;&amp; (showImgText || showImgTitle || showImg)\">\n            <input type=\"range\" inline=\"\" title=\"Delay\" name=\"delay\" min=\"1000\" max=\"5000\" step=\"1000\" value=\"3000\" condition=\"autoplay &amp;&amp; imagesAmount > 1 &amp;&amp; (showImgText || showImgTitle || showImg)\">\n        <input type=\"range\" inline=\"\" title=\"Slider Height\" name=\"slideHeight\" min=\"300\" max=\"700\" step=\"10\" value=\"480\" condition=\"showImgText || showImgTitle || showImg\">\n        <select title=\"Sliders Count\" name=\"imagesAmount\" condition=\"showImgText || showImgTitle || showImg\">\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected=\"\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\">8</option>\n        </select>\n        <input type=\"checkbox\" title=\"Show Buttons/Bullets\" name=\"showBullets\" condition=\"showImgText || showImgTitle || showImg\" checked=\"\">\n            <input type=\"color\" title=\"Color\" name=\"buttonsColor\" value=\"#273272\" condition=\"showImgText || showImgTitle || showImg\">\n        <input type=\"checkbox\" title=\"Image\" name=\"showImg\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showImgText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showImgTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Line Before Title\" name=\"lineTitle\" condition=\"showImgTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Padding\" name=\"testimonialsPadding\" condition=\"(showImgText || showImgTitle) &amp;&amp; showImg\" checked=\"\">\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"testimonialsBg\" condition=\"showImgText || showImgTitle\">\n        <input type=\"color\" title=\"Color\" name=\"textBg\" value=\"#ffffff\" condition=\"!testimonialsBg &amp;&amp; (showImgText || showImgTitle)\">\n        <select title=\"Align\" name=\"textImgAlign\" condition=\"showImgTitle || showImgText\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n    <header>Text</header>    \n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showCardSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showCardText\">\n        <input type=\"checkbox\" title=\"List\" name=\"showCardList\" checked=\"\">\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"listCount\" min=\"1\" max=\"9\" step=\"1\" value=\"3\" condition=\"showCardList\"> \n            <select title=\"Stylized Counters\" name=\"stylizedCounters\" condition=\"showCardList\">\n                <option value=\"1\">Counters</option>\n                <option value=\"2\">Markers</option>\n                <option value=\"3\" selected=\"\">Icons</option>\n            </select>\n            <input type=\"checkbox\" title=\"Border Bottom Counters\" name=\"countersBorder\" checked=\"\" condition=\"showCardList &amp;&amp; stylizedCounters == 1\">\n            <input type=\"range\" inline=\"\" title=\"Width Marker\" name=\"widthImg\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"3\" condition=\"showCardList\">\n            <input type=\"checkbox\" title=\"Title List\" name=\"showCardListTitle\" condition=\"showCardList\" checked=\"\">\n            <input type=\"checkbox\" title=\"Text List\" name=\"showCardListText\" condition=\"showCardList\" checked=\"\">\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showCardButtons\" checked=\"\">\n        <select title=\"Align\" name=\"cardAlign\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showCardButtons\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Padding\" name=\"showPadding\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showCardButtons\" checked=\"\">\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardTitle || showCardSubtitle || showCardText || showCardList || showCardButtons\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#f4f5f7\" condition=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardText || showCardList || showCardButtons)\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardText || showCardList || showCardButtons)\">\n    <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background2.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#f4f5f7\" selected=\"\">\n        </fieldset>\n        <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#f4f5f7\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"title-wrap mbr-pb-4\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-left': titleAlign==1,\n                         'align-center': titleAlign==2,\n                         'align-right': titleAlign==3, 'mbr-px-2': fullWidth}\">\n            <h1 class=\"mbr-section-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".title-wrap .mbr-section-title\">Slider Testimonials</h1>\n            <h2 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".title-wrap .mbr-section-subtitle\">Lorem ipsum dolor sit amet, consectetur adipisicing elit.</h2>\n        </div> \n        <div class=\"mbr-row\" mbr-class=\"{'mbr-px-2': !transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showCardButtons)}\">\n            <div class=\"elements-block mbr-flex flex-wrap mbr-jc-c\" mbr-class=\"{'mbr-row-reverse':reverseContent &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showCardButtons) &amp;&amp; (showImgText || showImgTitle || showImg), 'mbr-p-4': !transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showCardButtons)}\">\n                <div class=\"mbr-overlay\" mbr-if=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showCardButtons)\"></div>\n                <div class=\"second-element mbr-flex mbr-col-sm-12\" mbr-if=\"showCardTitle || showCardSubtitle || showCardText || showCardButtons || showCardList\" mbr-class=\"{'mbr-col-md-8':columnWidth==1 &amp;&amp; (showImgText || showImgTitle || showImg),\n                                     'mbr-col-md-7':columnWidth==2 &amp;&amp; (showImgText || showImgTitle || showImg),\n                                     'mbr-col-md-6':columnWidth==3 &amp;&amp; (showImgText || showImgTitle || showImg),\n                                     'mbr-col-md-5':columnWidth==4 &amp;&amp; (showImgText || showImgTitle || showImg),\n                                     'mbr-col-md-4':columnWidth==5 &amp;&amp; (showImgText || showImgTitle || showImg),\n                                     'mbr-col-md-12': !showImgText &amp;&amp; !showImgTitle &amp;&amp; !showImg}\">\n                        <div class=\"second-element-box mbr-m-auto\" mbr-class=\"{'align-center': cardAlign==2,\n                                 'align-right': cardAlign==3, 'mbr-px-2': transparentBg &amp;&amp; fullWidth}\">\n                        <h2 class=\"element-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showCardTitle\" data-app-selector=\".element-title\">Book Vishal for Your Next Event</h2>\n                        <h3 class=\"element-subtitle mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showCardSubtitle\" data-app-selector=\".element-subtitle\"><br>Make your Party Memorable with Live Music by Vishaal Gautam!</h3>\n                        <div class=\"mbr-list\" mbr-list=\"\" mbr-list-grow=\"listCount\" mbr-if=\"showCardList\" mbr-class=\"{'mbr-pt-4': showCardTitle || showCardSubtitle}\">\n                            <div class=\"mbr-flex items-list\" mbr-class=\"{'mbr-jc-c':cardAlign==2, 'mbr-jc-e': cardAlign==3}\">\n                                <div class=\"marker-wrap\" mbr-if=\"stylizedCounters == 1 || stylizedCounters == 2\"></div>\n                                <div class=\"icon-wrap\" mbr-if=\"stylizedCounters == 3\">\n                                    <span mbr-icon=\"\" class=\"amp-iconfont fa-birthday-cake fa\"></span>\n                                </div>\n                                <div class=\"list-box\" mbr-if=\"showCardListTitle || showCardListText\">\n                                    <h4 class=\"list-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" mbr-if=\"showCardListTitle\" data-app-selector=\".mbr-list .list-title\" mbr-class=\"{'mbr-pb-1':showCardListText}\">Small Events</h4>\n                                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardListText\" data-app-selector=\".mbr-list .mbr-text\">Intimate with just Vishaal and one accompanying pianist. Playlist based on audience&nbsp;requests with an open mic for other singers in audience.<br><br>Vishaal is very good at taking audiences on a memorable musical journey spanning Bollywood classics, most recent hits <br>and all the requests in-between!<br><br></p>\n                                 </div>\n                            </div> \n                            <div class=\"mbr-flex items-list\" mbr-class=\"{'mbr-jc-c':cardAlign==2, 'mbr-jc-e': cardAlign==3}\">\n                                <div class=\"marker-wrap\" mbr-if=\"stylizedCounters == 1 || stylizedCounters == 2\"></div>\n                                <div class=\"icon-wrap\" mbr-if=\"stylizedCounters == 3\">\n                                    <span mbr-icon=\"\" class=\"amp-iconfont fa-diamond fa\"></span>\n                                </div>\n                                <div class=\"list-box\" mbr-if=\"showCardListTitle || showCardListText\">\n                                    <h4 class=\"list-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" mbr-if=\"showCardListTitle\" data-app-selector=\".mbr-list .list-title\" mbr-class=\"{'mbr-pb-1':showCardListText}\">Large Events</h4>\n                                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardListText\" data-app-selector=\".mbr-list .mbr-text\">\n                                       A professional band with all the required talent and equipment to excel at any event.<br><br>Ask us to entertain your guests in the background while they meet and greet. Or ask us to deliver a musical performance your guests will be sure to record on their phones, share with their friends and post online.<br><br></p>\n                                 </div>\n                            </div>\n                        <div class=\"mbr-flex items-list\" mbr-class=\"{'mbr-jc-c':cardAlign==2, 'mbr-jc-e': cardAlign==3}\">\n                                <div class=\"marker-wrap\" mbr-if=\"stylizedCounters == 1 || stylizedCounters == 2\"></div>\n                                <div class=\"icon-wrap\" mbr-if=\"stylizedCounters == 3\">\n                                    <span mbr-icon=\"\" class=\"amp-iconfont fa-fire fa\"></span>\n                                </div>\n                                <div class=\"list-box\" mbr-if=\"showCardListTitle || showCardListText\">\n                                    <h4 class=\"list-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" mbr-if=\"showCardListTitle\" data-app-selector=\".mbr-list .list-title\" mbr-class=\"{'mbr-pb-1':showCardListText}\">\n                                        Devotional Events</h4>\n                                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardListText\" data-app-selector=\".mbr-list .mbr-text\">Sai Bhajan, Krishna Bhajan, Kabir Bhajan, Sunderkand, Shiv Bhajan or just Chanting - Vishaal will be humbled to sing his devotion at your next event.</p>\n                                 </div>\n                            </div></div>\n                        <p class=\"mbr-text element-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardText\" data-app-selector=\".element-text\" mbr-class=\"{'mbr-pt-3':showCardTitle || showCardSubtitle || showCardList}\">Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</p>\n                        <div mbr-buttons=\"\" class=\"mbr-section-btn\" mbr-class=\"{'mbr-pt-3':showCardTitle || showCardSubtitle || showCardList || showCardText}\" mbr-theme-style=\"display-7\" data-toolbar=\"-mbrBtnMove\" mbr-if=\"showCardButtons\"><a class=\"btn btn-md btn-primary-outline\" data-app-placeholder=\"Type Text\" href=\"https://wa.me/+918802279391\"><span class=\"fa fa-whatsapp mbr-iconfont mbr-iconfont-btn\"></span>BOOK NOW</a></div>\n                    </div>\n                </div>\n                <div class=\"image-wrap mbr-flex mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-8':columnWidth==5 &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showMilestone),\n                             'mbr-col-md-7':columnWidth==4 &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showMilestone),\n                             'mbr-col-md-6':columnWidth==3 &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showMilestone),\n                             'mbr-col-md-5':columnWidth==2 &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || sho